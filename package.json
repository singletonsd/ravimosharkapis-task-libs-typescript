    "reflect-metadata": "^0.1.3",
    "rxjs": "6.5.2",
    "rxjs-compat": "^6.5.3",
    "typescript": "3.2.4",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/compiler-cli": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "7.2.15",
    "ng-packagr": "10.1.1",
    "npm-check": "5.9.0",
    "npm-install-peers": "^1.2.1",
    "npm-snapshot": "^1.0.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "6.5.2",
    "typescript": "3.2.4",
    "zone.js": "0.10.2"
  },
  "dependencies": {
    "rxjs-compat": "^6.5.3"
  }
